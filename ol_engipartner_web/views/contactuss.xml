<odoo>
    <template id="contactuss" name="Contact Us">
        <t t-call="website.layout">
            <div class="oe_structure">
                <div class="container">
                    <form class="mt32 appointment_submit_form" t-attf-action="/visitordatasubmit2"  method="POST">
                          <section class="s_title parallax s_parallax_is_fixed bg-black-50 pt24 pb24" data-vcss="001"
                                 data-snippet="s_title" data-scroll-background-ratio="1" data-name="Title">
                            <span class="s_parallax_bg oe_img_bg"
                                  style="background-image: url('/web/image/website.s_banner_default_image'); background-position: 50% 0;"/>
                            <div class="o_we_bg_filter bg-black-50"/>
                            <div class="container">
                                <h1> Contact us</h1>
                            </div>
                        </section>
                        <center>
                            <hr style="height: 1px; border-width: 0; color: black; background-color: black;"/>
                            <p style="color: black; font-size:20px; ">We accelerate turnaround times, reduce soft costs
                                and improve profitability
                            </p>
                            <hr style="height: 1px; border-width: 0; color: black; background-color: black;"/>
                            <div class="progress">
                                <div class="progress-bar progress-bar-striped bg-success progress-bar-animated" role="progressbar" style="width: 100%"
                                     aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </center>
                          <div class="form-group">
                            <label class="" for="cntctfirstname">First Name:</label>
                            <div class="">
                                <input type="text" class="form-control" id="cntctfirstname"
                                       name="cntctfirstname" required="1"/>

                                <label class="" for="cntctlastname">Last Name:</label>
                            </div>
                            <div class="">
                                <input type="text" class="form-control" id="cntctlastname"
                                       name="cntctlastname" required="1"/>

                                <label class="" for="cntctcompanyname">Company Name:</label>
                            </div>
                            <div class="">
                                <input type="cntctcompanyname" class="form-control" id="cntctcompanyname"
                                       name="cntctcompanyname" required="1"/>

                                <label class="" for="cntctemailaddress">Email Address:</label>
                            </div>
                            <div class="">
                                <input type="text" class="form-control" id="cntctemailaddress"
                                       name="cntctemailaddress" required="1"/>

                            </div>
                              <label class="" for="cntctphoneno">Phone No:</label>

                            <div class="">
                                <input type="text" class="form-control" id="cntctphoneno"
                                       name="cntctphoneno" required="1"/>

                            </div>
                        </div>
                        <input type="checkbox" id="tick" name="tick" value="tick"/> <label for="html">By checking this box and clicking the "Done" button below, I verify that the number above is my email address and consent to receive emails via automated technology to this number regarding product and/or services offers by or on behalf of Engipartners. I also consent to receive telemarketing calls from or on behalf of Engipartners that may be auto-dialed. I agree to the Terms of Use and the Privacy Policy. </label>



                        <div class="row">
                            <div class="col-8"></div>
                            <div class="col-4 my-3">

                                <!--                        <button type="submit" class="btn btn-default" form="option" value="submit">Submit</button>-->

                                <button type="submit" class="btn btn-dark">SUBMIT</button>
                            </div>
                        </div>

                    </form>
                </div>
            </div>
        </t>
    </template>
</odoo>